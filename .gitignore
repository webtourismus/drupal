# Ignore dependencies that are managed with Composer.
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/
/drush/Commands/contrib/

# Ignore user generated content
/web/sites/*/files/
/private/files/
/web/webcam/
# except project-specific admin theme modifications
!/web/sites/*/files/gin-custom.css

# Ignore sensitive information
/web/sites/*/*settings*.php
/.env
/.env.*

# Ignore files generated by IDEs
.idea/
.vscode/

# Ignore OS and webhost generated files
.DS_Store
._*
Thumbs.ini
desktop.ini
/web/.well-known/

# Ignore temporary/vendor files from frontend building
.sass-cache/
/node_modules/
